{
    "name": "Chromex",
    "description": "This is sample extension",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_title": "Hi, I'm Chromex extension",
        "default_popup": "./popup/index.html"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js",
                "utils.js"
            ],
            "css": [
                "main.css"
            ],
            "matches": [
                "http://127.0.0.1/*",
                "https://zonepanel.dev.beepbeep.live/*"
            ]
        }
    ]
}